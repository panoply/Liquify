      "esm"
    ],
    "files": [
      "test/index.js"
    ],
    "concurrency": 5
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@liquify/eslint-config": "workspace:^1.0.0",
    "@liquify/prettier-config": "workspace:^1.0.0",
    "@liquify/rollup-utils": "workspace:^1.0.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@types/node": "^15.14.1",
    "ava": "^3.15.0",
    "chalk": "^4.1.1",
    "esm": "^3.2.25"
  }
