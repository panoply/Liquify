{
  "name": "@liquify/cryptographer",
  "version": "0.1.0",
  "description": "Encrypt and decrypt strings, numbers and objects to produce enigmatic string sequences",
  "created": "06-10-2020",
  "author": "ΝΙΚΟΛΑΣ ΣΑΒΒΙΔΗΣ <nicos@gmx.com>",
  "license": "MIT",
  "type": "module",
  "main": "package/index.js",
  "types": "package/index.d.ts",
  "module": "package/index.mjs",
  "exports": {
    "require": "./package/index.js",
    "import": "./package/index.mjs"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panoply/vscode-liquid.git"
  },
  "bugs": {
    "url": "https://github.com/panoply/vscode-liquid/issues"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "pack": "cd versions; npm pack ../ && cd ..",
    "test": "ava --watch --verbose"
  },
  "eslintConfig": {
    "extends": [
      "@liquify/eslint-config"
    ]
  },
  "ava": {
