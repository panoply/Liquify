{
  "name": "@liquify/rollup-plugin-cryptospec",
  "version": "0.1.0",
  "description": "Rollup plugin used by Liquify IDE editor tool for encryption and cryptography bundle processing",
  "author": "ΝΙΚΟΛΑΣ ΣΑΒΒΙΔΗΣ <nicos@gmx.com>",
  "license": "MIT",
  "type": "module",
  "types": "package/index.d.ts",
  "main": "package/index.js",
  "exports": {
    "require": "./package/index.js",
    "import": "./package/index.mjs"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panoply/liquify/master/scripts/rollup"
  },
  "bugs": {
    "url": "https://github.com/panoply/liquify/issues"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c --environment prod && node ./scripts/strip.mjs",
    "postpack": "cd versions; pnpm pack ../ && cd .."
  },
  "prettier": "@liquify/prettier-config",
  "eslintConfig": {
    "extends": [
      "@liquify/eslint-config"
    ]
  },
  "release": {
    "branches": [
      "master",
      "next"
    ]
  },
  "devDependencies": {
    "@liquify/eslint-config": "workspace:*",
    "@liquify/prettier-config": "workspace:*",
    "@liquify/rollup-utils": "workspace:*",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "chalk": "^4.1.1",
    "fs-extra": "^10.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-strip-code": "^0.2.7",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@liquify/cryptographer": "workspace:*",
    "@rollup/pluginutils": "^4.1.0",
    "rambda": "^6.7.0"
  },
  "optionalDependencies": {
    "rollup": "^2.70.0"
  }
}
