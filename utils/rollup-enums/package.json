{
  "name": "@liquify/rollup-enums",
  "version": "1.0.0",
  "description": "Converts string values to enum number values in Liquid Language Specs",
  "author": "ΝΙΚΟΛΑΣ ΣΑΒΒΙΔΗΣ <n.savvidis@gmx.com>",
  "license": "MIT",
  "type": "module",
  "types": "package/index.d.ts",
  "main": "package/index.js",
  "module": "package/index.mjs",
  "exports": {
    "require": "./package/index.js",
    "import": "./package/index.mjs"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panoply/liquify/master/build/rollup-enums"
  },
  "bugs": {
    "url": "https://github.com/panoply/liquify/issues"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "prepack": "rollup -c --environment prod",
    "pack": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --verbose"
  },
  "prettier": "@liquify/prettier-config",
  "eslintConfig": {
    "ignorePatterns": [
      "package/*"
    ],
    "extends": [
      "@liquify/eslint-config"
    ]
  },
  "devDependencies": {
    "@liquify/eslint-config": "workspace:*",
    "@liquify/prettier-config": "workspace:*",
    "@liquify/rollup-utils": "workspace:*",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.0"
  },
  "optionalDependencies": {
    "rollup": "^2.70.0"
  }
}
