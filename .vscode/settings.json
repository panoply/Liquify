{
  "editor.semanticHighlighting.enabled": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 96,
  "editor.detectIndentation": true,
  "editor.renderWhitespace": "none",
  "editor.renderIndentGuides": true,
  "editor.codeActionsOnSave": {
    "source.deleteSemicolons": true,
    "source.fixAll.eslint": true
  },
  "highlight.regexes": {
    "\\b(ScanState|TokenType|Characters)\\b\\.": {
      "regexFlags": "g",
      "filterLanguageRegex": "javascript",
      "filterFileRegex": ".*\\.js",
      "decorations": [
        {
          "color": "#81D4FA"
        }
      ]
    }
  },
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/node_modules": false,
    "**/package/node_modules": false,
    "**/versions": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "files.associations": {
    ".meta": "json",
    ".packages": "json",
    "*.sublime-settings": "jsonc",
    "*.sublime-commands": "jsonc"
  },
  "javascript.suggest.enabled": true,
  "javascript.suggestionActions.enabled": false,
  "javascript.validate.enable": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": false,
  "javascript.implicitProjectConfig.checkJs": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.suggest.autoImports": false,
  "eslint.alwaysShowStatus": true,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.validate": ["html", "javascript"],
  "eslint.options.extension": [".js"],
  "prettier.useEditorConfig": true,
  "prettier.requireConfig": true,
  "prettier.disableLanguages": ["html"],
  "vsicons.associations.files": [
    {
      "icon": "vscode",
      "extensions": [
        "extensions.json",
        "settings.json",
        "liquify.code-workspace"
      ],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "package",
      "extensions": [".packages.json"],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "config",
      "extensions": [
        ".lightserverrc",
        "argv.config.json",
        "build.config.json",
        "build.grammar.json",
        "package.config.json",
        ".pkgrc",
        "liquify.config.json",
        "pkg.config.js"
      ],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "babel2",
      "extensions": [".babelrc", "babel.config.json"],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "liquid",
      "extensions": [
        ".liquidrc",
        ".liquidrc.json",
        ".liquifyrc",
        ".liquifyrc.json"
      ],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "netlify",
      "extensions": ["_redirects", "_headers"],
      "filename": true,
      "format": "svg"
    }
  ]
}
