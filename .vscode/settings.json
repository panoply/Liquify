{
  /* -------------------------------------------- */
  /* PACKAGE MANAGER                              */
  /* -------------------------------------------- */
  "npm.packageManager": "pnpm",
  /* -------------------------------------------- */
  /* JSON SCHEMA STORES                           */
  /* -------------------------------------------- */
  "json.schemas": [
    {
      "fileMatch": [
        "package.json"
      ],
      "url": "https://unpkg.com/tsup/schema.json"
    },
    {
      "fileMatch": [
        "theme-docs-objects.json"
      ],
      "url": "./packages/schema/package/theme-docs/objects.json"
    }
  ],
  /* -------------------------------------------- */
  /* EXPLORER                                     */
  /* -------------------------------------------- */
  "explorer.fileNesting.patterns": {
    "LICENSE": "ThirdPartyNotices.txt",
    "*.ts": "${capture}.js",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml"
  },
  /* -------------------------------------------- */
  /* FILES                                        */
  /* -------------------------------------------- */
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.pnpm-debug.log": true,
    "**/pnpm-debug.log": true,
    "**/pnpm-lock.yaml": true,
    "**/.sass-cache": true,
    "**/.netlify": true
  },
  "files.associations": {
    ".meta": "json",
    ".packages": "json",
    "*.sublime-settings": "jsonc",
    "*.sublime-commands": "jsonc"
  },
  /* -------------------------------------------- */
  /* EDITOR SPECIFIC                              */
  /* -------------------------------------------- */
  "editor.semanticHighlighting.enabled": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "editor.detectIndentation": true,
  "editor.renderWhitespace": "none",
  "editor.codeActionsOnSave": {
    "source.deleteSemicolons": "never",
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  /* -------------------------------------------- */
  /* JAVASCRIPT                                   */
  /* -------------------------------------------- */
  "javascript.suggest.enabled": true,
  "javascript.suggestionActions.enabled": false,
  "javascript.validate.enable": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": false,
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  /* -------------------------------------------- */
  /* TYPESCRIPT                                   */
  /* -------------------------------------------- */
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.suggest.autoImports": false,
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  /* -------------------------------------------- */
  /* SCSS                                         */
  /* -------------------------------------------- */
  "scss.validate": false,
  "scss.scannerExclude": [
    "**/.git",
    "**/node_modules"
  ],
  /* -------------------------------------------- */
  /* ESLINT                                       */
  /* -------------------------------------------- */
  "eslint.packageManager": "pnpm",
  "eslint.alwaysShowStatus": true,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.lintTask.options": "--cache",
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  /* -------------------------------------------- */
  /* PRETTIER                                     */
  /* -------------------------------------------- */
  "prettier.useEditorConfig": true,
  "prettier.requireConfig": false,
  "prettier.ignorePath": ".prettierignore",
  /* -------------------------------------------- */
  /* STYLELINT                                    */
  /* -------------------------------------------- */
  "stylelint.validate": [
    "scss",
    "sass"
  ],
  "stylelint.packageManager": "pnpm",
  "stylelint.enable": true,
  /* -------------------------------------------- */
  /* ICONS ASSOCIATIONS                           */
  /* -------------------------------------------- */
  "vsicons.associations.files": [
    {
      "icon": "esbuild",
      "filename": true,
      "extensions": [
        "esbuild.config.js"
      ],
      "format": "svg"
    },
    {
      "icon": "eslint2",
      "extensions": [
        "eslint.rules.js"
      ],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "vscode",
      "extensions": [
        "extensions.json",
        "settings.json",
        "server.code-workspace"
      ],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "package",
      "extensions": [
        ".packages.json"
      ],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "vscode-insiders",
      "extensions": [
        "tsup.json"
      ],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "config",
      "extensions": [
        "package.config.json",
        "liquify.config.json"
      ],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "liquid",
      "extensions": [
        ".liquidrc",
        ".liquidrc.json"
      ],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "netlify",
      "extensions": [
        "_redirects",
        "_headers"
      ],
      "filename": true,
      "format": "svg"
    }
  ],
  /* -------------------------------------------- */
  /* CSPELL                                       */
  /* -------------------------------------------- */
  "cSpell.enableFiletypes": [
    "markdown"
  ],
  "cSpell.words": [
    "argsize",
    "bendcomment",
    "dryice",
    "endfor",
    "filesize",
    "Flems",
    "freemium",
    "fuckery",
    "iast",
    "iife",
    "jsonminify",
    "mailchimp",
    "mergerino",
    "multirepo",
    "noderesolve",
    "pluginutils",
    "pnpx",
    "Porsager",
    "prettifyrc",
    "prismjs",
    "Rasmus",
    "Roadmap",
    "siema",
    "srcset",
    "stickybits",
    "striplit",
    "stripliteral",
    "Stylelint",
    "turbolinks"
  ],
  "cSpell.ignoreWords": [
    "allman",
    "arname",
    "atstring",
    "attstore",
    "autoescape",
    "bcount",
    "blockinsert",
    "braccount",
    "braces",
    "camelcase",
    "cfelseif",
    "cfif",
    "cfset",
    "cftags",
    "cftransaction",
    "codium",
    "counta",
    "countb",
    "datanames",
    "datetime",
    "delim",
    "diffoutput",
    "dustatt",
    "earlyexit",
    "endcomment",
    "endslen",
    "endunless",
    "esctest",
    "eventsource",
    "fixsingleton",
    "harttle",
    "howmany",
    "htmlblocks",
    "htmlsings",
    "htmltable",
    "igcount",
    "ignoreme",
    "jscom",
    "jsxcount",
    "jsxquote",
    "keyend",
    "langstore",
    "lastchar",
    "linebreak",
    "lines",
    "liquidjs",
    "ltoke",
    "ltype",
    "metafields",
    "mvasilkov",
    "namelen",
    "nopush",
    "nosort",
    "outdent",
    "parncount",
    "pdcomment",
    "pjpg",
    "quotetest",
    "quux",
    "repath",
    "sgmlflag",
    "slashy",
    "stest",
    "strlen",
    "tablerow",
    "tempstore",
    "tname",
    "vs",
    "νικολας",
    "σαββιδης"
  ],
  /* -------------------------------------------- */
  /* REGEX HIGHIGHTS                              */
  /* -------------------------------------------- */
  "highlight.regexes": {
    "\\b(ScanState|TokenType|Characters)\\b\\.": {
      "regexFlags": "g",
      "filterLanguageRegex": "javascript",
      "filterFileRegex": ".*\\.[jt]s",
      "decorations": [
        {
          "color": "#81D4FA"
        }
      ]
    },
    "(\\$)\\b(seperator|filters|parameters|attributes)": {
      "regexFlags": "g",
      "filterLanguageRegex": "json",
      "filterFileRegex": ".*\\.json",
      "decorations": [
        {
          "color": "#888888"
        },
        {
          "color": "#888888"
        }
      ]
    },
    "(-{3})([\\s\\S]+?)(-{3})": {
      "regexFlags": "g",
      "filterLanguageRegex": "plaintext",
      "filterFileRegex": ".*\\.txt",
      "decorations": [
        {
          "color": "#81D4FA"
        },
        {
          "color": "#FFF78C",
          "fontStyle": "italic"
        },
        {
          "color": "#81D4FA"
        }
      ]
    },
    "(\"|\\||)([{(]|)(\\$)\\b([A-Za-z]*)\\b([)}]{0,}|)(\\?|)": {
      "regexFlags": "g",
      "filterLanguageRegex": "json",
      "filterFileRegex": ".*\\.json",
      "decorations": [
        {},
        {
          "color": "#3af59e"
        },
        {
          "color": "#81D4FA"
        },
        {
          "color": "#fd557f"
        },
        {
          "color": "#3af59e"
        },
        {
          "color": "#3af59e"
        }
      ]
    },
    "\"?([\\|+-])[{}]?(?=\\$)": {
      "regexFlags": "g",
      "filterLanguageRegex": "json",
      "filterFileRegex": ".*\\.json",
      "decorations": [
        {
          "color": "#ff9408",
          "border": "5px"
        }
      ]
    },
    "({[{%])([\\s\\S]*?)([%}]})": {
      "regexFlags": "g",
      "filterLanguageRegex": "plaintext",
      "filterFileRegex": ".*\\.md",
      "decorations": [
        {
          "color": "#ff9408"
        },
        {
          "color": "#888888"
        },
        {
          "color": "#ff9408"
        }
      ]
    }
  }
}
