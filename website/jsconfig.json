{
  "exclude": ["node_modules/**"],
  "include": [
    "package",
    "scripts",
    "website",
    "server",
    "specs",
    "package/clients",
    "export/vscode-liquify",
    "packages, "specs"/clients", "cli"
  ],
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "checkJs": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noEmit": true,
    "noEmitOnError": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true
  }
}
