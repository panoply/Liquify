{
  "name": "@liquify/web",
  "version": "0.0.0",
  "description": "Liquify IDE Website",
  "author": "Nikolas Savvidis <nicos@gmx.com>",
  "license": "ISC",
  "main": ".eleventy.js",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.5.3",
    "countup.js": "^2.0.7",
    "freezeframe": "^4.1.3",
    "mithril": "^2.0.4",
    "stickybits": "^3.7.8",
    "stimulus": "^2.0.0",
    "turbolinks": "^5.2.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eslint": "^7.17.0",
    "eslint-plugin-babel": "^5.3.1",
    "light-server": "^2.9.1",
    "mkdirp": "^1.0.4",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.0",
    "stylelint": "^13.8.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-scss": "^3.18.0",
    "svgo": "^1.3.2"
  },
  "scripts": {
    "dev": "npm-run-all clean make --parallel styles 11ty rollup",
    "11ty:build": "pnpx eleventy",
    "11ty": "pnpx eleventy --serve --watch",
    "rollup": "rollup -c -w",
    "rollup:prod": "rollup -c --environment prod",
    "styles": "sass src/style/stylesheet.scss public/assets/stylesheet.css --watch --load-path=./node_modules/ && postcss ./public/assets/*.css --replace",
    "styles:prod": "sass ./src/style/ public/assets/ && postcss public/assets/*.css --replace",
    "make": "mkdirp ./public && mkdirp ./public/assets && mkdirp ./public/assets",
    "clean": "rimraf public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "url": "https://github.com/panoply/liquify.dev"
  },
  "peerDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.2.0",
    "prettier": "^2.0.5",
    "rollup": "^2.15.0",
    "stylelint": "^13.6.0"
  },
  "packages": {
    "project": {
      "path": ".",
      "name": "liquify",
      "repo": "github.com/panoply/liquify",
      "version": "0.0.0-development"
    },
    "./cryptographer": {
      "path": "build/cryptographer",
      "name": "@liquify/cryptographer",
      "repo": "github.com/panoply/vscode-liquid.git",
      "version": "0.1.0"
    },
    "./cryptospec": {
      "path": "build/rollup-plugins/cryptospec",
      "name": "@liquify/rollup-plugin-cryptospec",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.1.0"
    },
    "./globs": {
      "path": "build/rollup-plugins/globs",
      "name": "@liquify/rollup-plugin-globs",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.0.0"
    },
    "./obfuscator": {
      "path": "build/rollup-plugins/obfuscator",
      "name": "@liquify/rollup-plugin-obfuscator",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.0.0"
    },
    "./utils": {
      "path": "build/rollup-plugins/utils",
      "name": "@liquify/rollup-plugin-utils",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.0.0"
    },
    "./atom": {
      "path": "packages/clients/atom",
      "name": "atom-liquid",
      "repo": "github.com/panoply/liquify",
      "version": "0.1.0"
    },
    "./vscode": {
      "path": "packages/clients/vscode",
      "name": "liquify-vscode",
      "repo": "github.com/panoply/vscode-liquid.git",
      "version": "0.0.1"
    },
    "./licensing": {
      "path": "packages/licensing",
      "name": "@liquify/licensing",
      "repo": "github.com/panoply/vscode-liquid.git",
      "version": "0.0.0"
    },
    "./server": {
      "path": "packages/server",
      "name": "@liquify/liquid-language-server",
      "repo": "github.com/panoply/liquify",
      "version": "0.0.0"
    },
    "./specs": {
      "path": "packages/specs",
      "name": "@liquify/liquid-language-specs",
      "repo": "github.com/panoply/liquify",
      "version": "0.0.0"
    },
    "./web": {
      "path": "web",
      "name": "@liquify/web",
      "repo": "github.com/panoply/liquify.dev",
      "version": "0.0.0"
    }
  }
}