{
  "name": "@liquify/cli",
  "version": "0.0.1",
  "description": "Interactive development CLI for the Liquify extension/package",
  "author": "Nikolas Savvidis <nicos@gmx.com>",
  "license": "MIT",
  "bin": {
    "liquify": "./bin/liquify"
  },
  "main": "./package/liquify-cli.cjs.js",
  "module": "package/liquify-cli.es.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panoply/vscode-liquid.git"
  },
  "bugs": {
    "url": "https://github.com/panoply/vscode-liquid/issues"
  },
  "scripts": {
    "liquify": "./bin/liquify.js",
    "cli": "node package/index.js",
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "tests": "node ./test/require-specs.js",
    "pack": "cd package; pnpm pack ../ && cd ..",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "release": {
    "branches": [
      "master",
      "next"
    ]
  },
  "dependencies": {
    "@liquify/specs": "file:../packages/specs/versions/liquify-specs-0.0.1.tgz",
    "boxen": "^4.2.0",
    "chalk": "^4.0.0",
    "chokidar": "^3.4.0",
    "console-clear": "^1.1.1",
    "cryptr": "^6.0.2",
    "execa": "^4.0.0",
    "figlet": "^1.4.0",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "json-to-pretty-yaml": "^1.2.2",
    "jsonminify": "^0.4.1",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "parse-git-config": "^3.0.0",
    "strip-json-comments": "^3.1.0"
  },
  "optionalDependencies": {
    "fx": "^18.0.1"
  },
  "packages": {
    "root": {
      "liquify": "."
    },
    "atom": {
      "atom-liquid": "packages/clients/atom"
    },
    "vscode": {
      "liquify-vscode": "packages/clients/vscode"
    },
    "grammar": {
      "@liquify/liquid-language-grammars": "packages/grammar"
    },
    "schema": {
      "@liquify/schema-stores": "packages/schema"
    },
    "server": {
      "liquid-language-server": "packages/server"
    },
    "specs": {
      "@liquify/specs": "packages/specs"
    },
    "cdn": {
      "@liquify/cdn": "web/cdn"
    },
    "docs": {
      "@liquify/docs": "web/docs"
    }
  }
}