{
  "name": "@liquify/path-resolve",
  "version": "0.0.0",
  "description": "Path resolver for the Liquify monorepo",
  "author": "Nikolas Savvidis <nicos@gmx.com>",
  "license": "MIT",
  "main": "./package/index.cjs.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panoply/vscode-liquid.git"
  },
  "bugs": {
    "url": "https://github.com/panoply/vscode-liquid/issues"
  },
  "scripts": {
    "_watch": "rollup -c -w",
    "_build": "rollup -c --environment prod",
    "prebuild": "rollup -i index.js -o package/index.cjs.js -f cjs --environment prod",
    "pack": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --watch --verbose"
  },
  "ava": {
    "babel": true,
    "files": [
      "test/**/*"
    ],
    "concurrency": 5
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@types/node": "^14.0.5",
    "ava": "^3.8.2",
    "chalk": "^4.0.0",
    "rollup-plugin-terser": "^6.0.1"
  },
  "dependencies": {
    "cryptorjs": "^3.0.0",
    "find-up": "^4.1.0"
  }
}
