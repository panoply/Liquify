{
  "name": "@liquify/cryptographer",
  "version": "0.0.0",
  "description": "Encrypt and decrypt strings, numbers and objects",
  "author": "Nikolas Savvidis <nicos@gmx.com>",
  "license": "MIT",
  "created": "06-10-2020",
  "main": "package/index.cjs.js",
  "types": "index.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panoply/vscode-liquid.git"
  },
  "bugs": {
    "url": "https://github.com/panoply/vscode-liquid/issues"
  },
  "scripts": {
    "watch": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "prebuild": "rollup -i index.js -o package/index.cjs.js -f cjs --environment prod",
    "pack": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --watch --verbose"
  },
  "ava": {
    "require": [
      "esm",
      "@babel/register"
    ],
    "babel": true,
    "files": [
      "test/**/*"
    ],
    "concurrency": 5
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@liquify/rollup-plugin-obfuscator": "workspace:^0.0.0",
    "@liquify/rollup-plugin-utils": "workspace:^0.0.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@types/node": "^14.14.16",
    "ava": "^3.14.0",
    "chalk": "^4.1.0",
    "esm": "^3.2.25",
    "rollup-plugin-terser": "^7.0.2"
  }
}
