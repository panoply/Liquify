{
  "name": "@liquify/rollup-plugin-cryptospec",
  "version": "0.1.0",
  "description": "Rollup plugin used by Liquify IDE editor tool for encryption and cryptography bundle processing",
  "author": "ΝΙΚΟΛΑΣ ΣΑΒΒΙΔΗΣ <nicos@gmx.com>",
  "license": "MIT",
  "type": "module",
  "types": "package/index.d.ts",
  "main": "package/index.js",
  "exports": {
    "require": "./package/index.js",
    "import": "./package/index.mjs"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panoply/liquify/master/scripts/rollup"
  },
  "bugs": {
    "url": "https://github.com/panoply/liquify/issues"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c --environment prod && node ./scripts/strip.mjs",
    "tests": "node ./test/require-specs.js",
    "pack": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --verbose"
  },
  "esm": {
    "cjs": true
  },
  "eslintConfig": {
    "extends": [
      "@liquify/eslint-config-js"
    ]
  },
  "ava": {
    "babel": true,
    "files": [
      "test/**/**"
    ],
    "concurrency": 5
  },
  "release": {
    "branches": [
      "master",
      "next"
    ]
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@liquify/eslint-config-js": "workspace:^1.0.0",
    "@liquify/rollup-plugin-utils": "workspace:^0.1.1",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "ava": "^3.15.0",
    "chalk": "^4.1.1",
    "esm": "^3.2.25",
    "fs-extra": "^10.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-strip-code": "^0.2.7",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@liquify/cryptographer": "workspace:^0.1.0",
    "@rollup/pluginutils": "^4.1.0",
    "rambda": "^6.7.0"
  }
}
