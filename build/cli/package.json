{
  "name": "@liquify/cli",
  "version": "0.0.0",
  "description": "Interactive development CLI for the Liquify extension/package",
  "author": "Nikolas Savvidis <nicos@gmx.com>",
  "license": "MIT",
  "bin": {
    "liquify": "./bin/liquify"
  },
  "main": "./package/liquify-cli.cjs.js",
  "module": "package/liquify-cli.es.js",
  "types": "types/index.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panoply/vscode-liquid.git"
  },
  "bugs": {
    "url": "https://github.com/panoply/vscode-liquid/issues"
  },
  "scripts": {
    "liquify": "./bin/liquify",
    "cli": "node package/index.js",
    "dev": "rollup -c -w",
    "_build": "rollup -c",
    "tests": "node ./test/require-specs.js",
    "prepack": "rollup -c --environment prod",
    "pack": "cd versions; pnpm pack ../ && cd ..",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "release": {
    "branches": [
      "master",
      "next"
    ]
  },
  "dependencies": {
    "@liquify/liquid-language-specs": "workspace:^0.0.0",
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.2",
    "console-clear": "^1.1.1",
    "cryptr": "^6.0.2",
    "execa": "^4.0.3",
    "execution-time": "^1.4.1",
    "figlet": "^1.5.0",
    "find-up": "^5.0.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "json-to-pretty-yaml": "^1.2.2",
    "jsonminify": "^0.4.1",
    "minimist": "^1.2.5",
    "parse-git-config": "^3.0.0",
    "strip-json-comments": "^3.1.1"
  },
  "optionalDependencies": {
    "fx": "^20.0.2"
  },
  "devDependencies": {
    "rollup-plugin-terser": "^7.0.2"
  }
}
