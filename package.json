{
  "name": "liquify",
  "version": "0.0.0-development",
  "description": "Liquid Templating Language Support and IDE tooling",
  "author": "Nikolas Savvidis <nicos@gmx.com>",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "engines": {
    "pnpm": ">=5"
  },
  "scripts": {
    "dev": "pnpm run watch -r --stream  --filter ./packages/ --no-sort --workspace-concurrency Infinity",
    "dev:build": "pnpm run watch -r --filter ./build/ --stream --no-sort --workspace-concurrency Infinity",
    "build:build": "pnpm run build -r --filter ./build/ --stream --no-sort --workspace-concurrency Infinity",
    "semantic-release": "semantic-release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "post-merge": "pnpm run push"
    }
  },
  "repository": {
    "url": "https://github.com/panoply/liquify"
  },
  "packages": {
    "project": {
      "path": ".",
      "name": "liquify",
      "repo": "github.com/panoply/liquify",
      "version": "0.0.0-development"
    },
    "./cli": {
      "path": "build/cli",
      "name": "@liquify/cli",
      "repo": "github.com/panoply/vscode-liquid.git",
      "version": "0.0.0"
    },
    "./cryptographer": {
      "path": "build/cryptographer",
      "name": "@liquify/cryptographer",
      "repo": "github.com/panoply/vscode-liquid.git",
      "version": "0.0.0"
    },
    "./cryptospec": {
      "path": "build/rollup-plugins/cryptospec",
      "name": "@liquify/rollup-plugin-cryptospec",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.0.0"
    },
    "./globs": {
      "path": "build/rollup-plugins/globs",
      "name": "@liquify/rollup-plugin-globs",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.0.0"
    },
    "./obfuscator": {
      "path": "build/rollup-plugins/obfuscator",
      "name": "@liquify/rollup-plugin-obfuscator",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.0.0"
    },
    "./supplant": {
      "path": "build/rollup-plugins/supplant",
      "name": "@liquify/rollup-plugin-supplant",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.0.0"
    },
    "./utils": {
      "path": "build/rollup-plugins/utils",
      "name": "@liquify/rollup-plugin-utils",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.0.0"
    },
    "./testae": {
      "path": "build/testae",
      "name": "@liquify/testae",
      "repo": "github.com/panoply/vscode-liquid.git",
      "version": "0.0.0"
    },
    "./atom": {
      "path": "packages/clients/atom",
      "name": "atom-liquid",
      "repo": "github.com/panoply/liquify",
      "version": "0.1.0"
    },
    "./vscode": {
      "path": "packages/clients/vscode",
      "name": "liquify-vscode",
      "repo": "github.com/panoply/vscode-liquid.git",
      "version": "0.0.1"
    },
    "./parser": {
      "path": "packages/parser",
      "name": "@liquify/liquid-parser",
      "repo": "github.com/panoply/liquid-parser",
      "version": "0.0.0"
    },
    "./schema": {
      "path": "packages/schema",
      "name": "@liquify/schema-stores",
      "repo": "github.com/panoply/liquify",
      "version": "0.0.1"
    },
    "./server": {
      "path": "packages/server",
      "name": "@liquify/liquid-language-server",
      "repo": "github.com/panoply/liquify",
      "version": "0.0.0"
    },
    "./specs": {
      "path": "packages/specs",
      "name": "@liquify/liquid-language-specs",
      "repo": "github.com/panoply/liquify",
      "version": "0.0.0"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.10.5",
    "@liquify/rollup-plugin-globs": "workspace:^0.0.0",
    "@liquify/rollup-plugin-utils": "workspace:^0.0.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/pluginutils": "^3.1.0",
    "@types/lodash": "^4.14.158",
    "@types/mocha": "^8.0.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "console-clear": "^1.1.1",
    "cryptorjs": "^3.0.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "execa": "^4.0.3",
    "figlet": "^1.5.0",
    "fs-extra": "^9.0.1",
    "husky": "^4.2.5",
    "inquirer": "^7.3.3",
    "javascript-obfuscator": "^1.7.0",
    "jsonminify": "^0.4.1",
    "lodash": "^4.17.19",
    "mocha": "^8.0.1",
    "parse-git-config": "^3.0.0",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "qs": "^6.9.4",
    "rollup": "^2.23.0",
    "rollup-plugin-javascript-obfuscator": "^1.0.4",
    "rollup-plugin-terser": "^6.1.0",
    "semantic-release": "^17.1.1",
    "strip-json-comments": "^3.1.1"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
