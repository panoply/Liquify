{
  "name": "liquify",
  "version": "0.0.0-development",
  "description": "Liquid Templating Language Support and IDE tooling",
  "author": "Nikolas Savvidis <nicos@gmx.com>",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "engines": {
    "pnpm": ">=4"
  },
  "scripts": {
    "liquify": "liquify",
    "dev": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "cli:dev": "pnpm run dev --filter ./build/cli/",
    "client:atom": "pnpm run build --filter ./packages/clients/atom/",
    "client:sublime": "pnpm run build --filter ./packages/clients/sublime/",
    "vscode": "pnpm run dev --filter ./packages/clients/vscode/",
    "vscode:build": "pnpm run dev --filter ./packages/clients/vscode/",
    "server": "pnpm run dev --filter ./packages/server/",
    "server:build": "pnpm run build --filter ./packages/server/",
    "grammar:build": "pnpm run build --filter ./packages/grammar/",
    "grammar:create": "pnpm run create --filter ./packages/grammar/",
    "grammar:watch": "pnpm run watch --filter ./packages/grammar/",
    "specs:peek": "pnpm run peek --filter ./packages/specs/",
    "specs:build": "pnpm run build --filter ./packages/specs/",
    "specs:watch": "pnpm run watch --filter ./packages/specs/",
    "syntax": "node packages/specs/index.js",
    "semantic-release": "semantic-release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "post-merge": "pnpm run push"
    }
  },
  "repository": {
    "url": "https://github.com/panoply/liquify"
  },
  "packages": {
    "project": {
      "path": ".",
      "name": "liquify",
      "repo": "github.com/panoply/liquify",
      "version": "0.0.0-development"
    },
    "./cli": {
      "path": "build/cli",
      "name": "@liquify/cli",
      "repo": "github.com/panoply/vscode-liquid.git",
      "version": "0.0.0"
    },
    "./path-resolve": {
      "path": "build/path-resolve",
      "name": "@liquify/path-resolve",
      "repo": "github.com/panoply/vscode-liquid.git",
      "version": "0.0.0"
    },
    "./globs": {
      "path": "build/rollup-plugins/globs",
      "name": "@liquify/rollup-plugin-globs",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.0.0"
    },
    "./supplant": {
      "path": "build/rollup-plugins/supplant",
      "name": "@liquify/rollup-plugin-supplant",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.0.0"
    },
    "./utils": {
      "path": "build/rollup-plugins/utils",
      "name": "@liquify/rollup-plugin-utils",
      "repo": "github.com/panoply/liquify/master/scripts/rollup",
      "version": "0.0.0"
    },
    "./atom": {
      "path": "packages/clients/atom",
      "name": "atom-liquid",
      "repo": "github.com/panoply/liquify",
      "version": "0.1.0"
    },
    "./vscode": {
      "path": "packages/clients/vscode",
      "name": "liquify-vscode",
      "repo": "github.com/panoply/vscode-liquid.git",
      "version": "0.0.1"
    },
    "./schema": {
      "path": "packages/schema",
      "name": "@liquify/schema-stores",
      "repo": "github.com/panoply/liquify",
      "version": "0.0.1"
    },
    "./server": {
      "path": "packages/server",
      "name": "liquid-language-server",
      "repo": "github.com/panoply/liquify",
      "version": "0.0.0"
    },
    "./specs": {
      "path": "packages/specs",
      "name": "@liquify/liquid-language-specs",
      "repo": "github.com/panoply/liquify",
      "version": "0.0.0"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.6",
    "@liquify/path-resolve": "workspace:^0.0.0",
    "@liquify/rollup-plugin-utils": "workspace:^0.0.0",
    "@liquify/rollup-plugin-globs": "workspace:^0.0.0",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/pluginutils": "^3.0.10",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.8",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "console-clear": "^1.1.1",
    "cryptorjs": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "execa": "^4.0.1",
    "figlet": "^1.4.0",
    "fs-extra": "^9.0.0",
    "husky": "^4.2.5",
    "inquirer": "^7.1.0",
    "javascript-obfuscator": "^0.28.1",
    "jsonminify": "^0.4.1",
    "lodash": "^4.17.15",
    "mocha": "^7.1.2",
    "parse-git-config": "^3.0.0",
    "pnpm": "^4.14.2",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.1",
    "qs": "^6.9.4",
    "rollup": "^2.10.5",
    "rollup-plugin-javascript-obfuscator": "^1.0.4",
    "rollup-plugin-terser": "^5.3.0",
    "semantic-release": "^17.0.7",
    "strip-json-comments": "^3.1.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}