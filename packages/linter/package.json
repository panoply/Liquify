{
  "name": "@liquify/liquid-language-server",
  "version": "0.0.0",
  "description": "Language Server for Liquid Templating Language",
  "scripts": {
    "watch": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "rollup": "rollup -c",
    "pack": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --color --watch",
    "test:parser": "ava --color --verbose --watch"
  },
  "private": false,
  "author": "",
  "owner": "SISSEL | ΝΙΚΟΛΑΣ ΣΑΒΒΙΔΗΣ",
  "license": "see LICENSE.txt",
  "main": "./server.js",
  "bundledDependencies": [
    "vscode-css-languageservice",
    "vscode-languageserver"
  ],
  "ava": {
    "require": [
      "esm",
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "babelrc": true,
        "configFile": false
      }
    },
    "files": [
      "test/parsing/parser.js"
    ],
    "concurrency": 5
  },
  "dependencies": {
    "@liquify/cryptographer": "workspace:^0.1.0",
    "@liquify/liquid-language-specs": "workspace:^0.0.0",
    "fs-extra": "^9.0.1",
    "netlicensing-client": "^1.2.24",
    "prettydiff": "^101.2.6",
    "strip-json-comments": "^3.1.1",
    "vscode-css-languageservice": "^5.0.2",
    "vscode-emmet-helper": "^2.2.2",
    "vscode-html-languageservice": "^4.0.0",
    "vscode-json-languageservice": "^4.0.0",
    "vscode-languageserver": "^7.0.0",
    "vscode-languageserver-textdocument": "^1.0.1",
    "vscode-languageserver-types": "^3.16.0",
    "vscode-uri": "^3.0.1",
    "yamljs": "^0.3.0"
  },
  "repository": {
    "url": "https://github.com/panoply/liquify"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@ava/babel-preset-stage-4": "^4.0.0",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/register": "^7.12.10",
    "@liquify/rollup-plugin-globs": "workspace:^0.0.0",
    "@liquify/rollup-plugin-obfuscator": "workspace:^0.0.0",
    "@liquify/rollup-plugin-utils": "workspace:^0.0.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "ava": "^3.15.0",
    "babel-plugin-lodash": "^3.3.4",
    "chalk": "^4.1.0",
    "cli-table": "^0.3.4",
    "colm": "^0.1.1",
    "copy-node-modules": "^1.1.1",
    "esm": "^3.2.25",
    "marky": "^1.2.1",
    "pretty-ms": "^7.0.1",
    "prettyoutput": "^1.2.0",
    "rollup-plugin-filesize": "^9.1.0"
  }
}
