{
  "name": "@liquify/licensing",
  "version": "0.0.0",
  "description": "Liquify Licensing",
  "author": "Nikolas Savvidis <nicos@gmx.com>",
  "license": "SEE LICENSE IN LICENSE",
  "main": "./package/index.cjs.js",
  "module": "./package/index.es.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/panoply/vscode-liquid.git"
  },
  "bugs": {
    "url": "https://github.com/panoply/vscode-liquid/issues"
  },
  "scripts": {
    "watch": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "prebuild": "rollup -i index.js -o package/index.cjs.js -f cjs --environment prod",
    "pack": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --watch --verbose"
  },
  "ava": {
    "require": [
      "esm",
      "@babel/register"
    ],
    "babel": true,
    "files": [
      "test/**/*"
    ],
    "concurrency": 5
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@liquify/rollup-plugin-obfuscator": "workspace:^0.0.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@types/node": "^14.11.1",
    "ava": "^3.12.1",
    "chalk": "^4.1.0",
    "esm": "^3.2.25",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@liquify/cryptographer": "workspace:^0.0.0",
    "netlicensing-client": "^1.2.24"
  }
}
