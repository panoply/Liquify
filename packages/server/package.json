{
  "name": "@liquify/liquid-language-server",
  "version": "0.0.0",
  "description": "Language Server for Liquid Templating Language",
  "scripts": {
    "watch": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "rollup": "rollup -c",
    "pack": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --verbose --watch"
  },
  "private": false,
  "author": "",
  "owner": "B.V SISSEL and/or ΝΙΚΟΛΑΣ ΣΑΒΒΙΔΗΣ | NIKOLAS SAVVIDIS",
  "license": "ISC",
  "main": "./server.js",
  "bundledDependencies": [
    "vscode-css-languageservice",
    "vscode-languageserver"
  ],
  "ava": {
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false,
        "configFile": false
      }
    },
    "files": [
      "test/**/**"
    ],
    "concurrency": 5
  },
  "dependencies": {
    "@liquify/cryptographer": "workspace:^0.0.0",
    "@liquify/liquid-language-specs": "workspace:^0.0.0",
    "fs-extra": "^9.0.0",
    "netlicensing-client": "^1.2.21",
    "prettydiff": "^101.2.6",
    "ramda": "^0.27.0",
    "strip-json-comments": "^3.1.0",
    "vscode-css-languageservice": "^4.1.1",
    "vscode-emmet-helper": "^1.2.17",
    "vscode-html-languageservice": "^3.0.3",
    "vscode-json-languageservice": "^3.5.1",
    "vscode-languageserver": "^6.1.1",
    "vscode-languageserver-textdocument": "^1.0.1",
    "vscode-uri": "^2.1.1",
    "yamljs": "^0.3.0"
  },
  "repository": {
    "url": "https://github.com/panoply/liquify"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@ava/babel-preset-stage-4": "^4.0.0",
    "@babel/plugin-proposal-private-methods": "^7.10.1",
    "@babel/register": "^7.10.1",
    "@liquify/rollup-plugin-globs": "workspace:^0.0.0",
    "@liquify/rollup-plugin-obfuscator": "workspace:^0.0.0",
    "@liquify/rollup-plugin-utils": "workspace:^0.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "ava": "^3.8.2",
    "babel-plugin-ramda": "^2.0.0",
    "copy-node-modules": "^1.1.1",
    "marky": "^1.2.1",
    "pretty-ms": "^7.0.0",
    "rollup-plugin-filesize": "^9.0.1"
  }
}
