{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "node": 12
        }
      }
    ],
    ["@babel/preset-typescript"],
    ["const-enum"]
  ],
  "sourceMaps": true,
  "inputSourceMap": true,
  "retainLines": true,
  "plugins": [
    ["babel-plugin-lodash"],
    ["@babel/plugin-proposal-optional-chaining"],
    ["@babel/plugin-proposal-class-properties"],
    ["@babel/plugin-syntax-class-properties"],
    ["@babel/plugin-transform-runtime"],
    [
      "const-enum",
      {
        "transform": "constObject"
      }
    ]
  ]
}
