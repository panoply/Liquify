{
  "name": "@liquify/liquid-language-specs",
  "version": "0.1.2",
  "private": false,
  "author": {
    "name": "Νίκος Σαβίδης",
    "email": "n.savvidis@gmx.com"
  },
  "contributors": [
    {
      "name": "Joseph Curtis",
      "email": "joecurt85@gmail.com"
    }
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "description": "Liquid language variation specification references used by Liquify",
  "main": "./package/index.js",
  "types": "./package/@types/index.d.ts",
  "exports": {
    "require": "./package/index.js",
    "import": "./package/index.js"
  },
  "engines": {
    "pnpm": ">=4",
    "node": ">=12"
  },
  "homepage": "https://liquify.dev",
  "repository": {
    "url": "https://github.com/panoply/liquify"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "html": "node ./scripts/html:generate.js",
    "package": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava ./test/* --watch --verbose --color",
    "test:cases": "ava ./test/cases/* --verbose --color"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "concurrency": 5
  },
  "eslintConfig": {
    "extends": [
      "@liquify/eslint-config-ts"
    ],
    "rules": {
      "no-template-curly-in-string": "off",
      "no-unused-vars": "off",
      "no-control-regex": "off",
      "prefer-const": "off"
    }
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@liquify/eslint-config-ts": "workspace:^1.0.1",
    "@liquify/rollup-plugin-obfuscator": "workspace:^0.2.0",
    "@liquify/rollup-plugin-utils": "workspace:^0.1.1",
    "@liquify/tsconfig": "workspace:^1.0.0",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "ava": "^3.15.0",
    "esm": "^3.2.25",
    "rollup": "^2.53.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-watch-assets": "^1.0.1",
    "typescript": "^4.3.5",
    "vscode-languageserver": "^7.0.0",
    "vscode-languageserver-types": "^3.16.0",
    "vscode-web-custom-data": "^0.3.4"
  },
  "peerDependencies": {
    "rollup": "^2.53.0",
    "typescript": "^4.3.5"
  }
}
