{
  "name": "@liquify/liquid-language-specs",
  "version": "0.1.2",
  "created": "2020",
  "private": false,
  "author": "ΝΙΚΟΛΑΣ ΣΑΒΒΙΔΗΣ <nicos@gmx.com>",
  "contributors": ["Νίκος Σαβίδης", "Joseph Curtis"],
  "license": "SEE LICENSE IN LICENSE.txt",
  "description": "Liquid language variation specification references used by Liquify",
  "main": "package/index.js",
  "types": "package/index.d.ts",
  "exports": {
    "require": "./package/index.js"
  },
  "engines": {
    "pnpm": ">=4",
    "node": ">=12"
  },
  "repository": {
    "url": "https://github.com/panoply/liquify"
  },
  "eslintConfig": {
    "extends": ["@liquify/eslint-config-ts"],
    "rules": {
      "no-template-curly-in-string": "off",
      "no-unused-vars": "off",
      "no-control-regex": "off"
    }
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "html": "node ./scripts/html-data.js",
    "pkg": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --watch --verbose",
    "test:log": "ava --verbose -- logs"
  },
  "ava": {
    "require": ["esm"],
    "files": ["test/dev.test.js"],
    "concurrency": 5
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@liquify/eslint-config-ts": "workspace:^1.0.1",
    "@liquify/rollup-plugin-cryptospec": "workspace:^0.1.0",
    "@liquify/rollup-plugin-obfuscator": "workspace:^0.2.0",
    "@liquify/rollup-plugin-utils": "workspace:^0.1.1",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "ava": "^3.15.0",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "lodash.has": "^4.5.2",
    "object-sizeof": "^1.6.1",
    "pretty-bytes": "^5.6.0",
    "rambda": "^6.7.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-watch-assets": "^1.0.1",
    "vscode-languageserver-types": "^3.16.0",
    "vscode-web-custom-data": "^0.3.4"
  },
  "dependencies": {
    "@liquify/cryptographer": "workspace:^0.1.0"
  }
}
