{
  "name": "@liquify/liquid-language-specs",
  "version": "0.1.2",
  "created": "2020",
  "private": false,
  "author": "ΝΙΚΟΛΑΣ ΣΑΒΒΙΔΗΣ <nicos@gmx.com>",
  "contributors": [
    "Νίκος Σαβίδης",
    "Joseph Curtis"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "description": "Liquid language variation specification references used by Liquify",
  "main": "package/index.js",
  "types": "package/index.d.ts",
  "exports": {
    "require": "./package/index.js",
    "import": "./package/index.es.js"
  },
  "engines": {
    "pnpm": ">=4",
    "node": ">=12"
  },
  "repository": {
    "url": "https://github.com/panoply/liquify"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "pkg": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --verbose",
    "test:log": "ava --verbose -- logs"
  },
  "ava": {
    "require": [
      "esm",
      "@babel/register"
    ],
    "files": [
      "test/**/**"
    ],
    "concurrency": 5
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@liquify/rollup-plugin-cryptospec": "workspace:^0.1.0",
    "@liquify/rollup-plugin-globs": "workspace:^0.1.0",
    "@liquify/rollup-plugin-obfuscator": "workspace:^0.2.0",
    "@liquify/rollup-plugin-utils": "workspace:^0.1.0",
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "ava": "^3.15.0",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "object-sizeof": "^1.6.1",
    "pretty-bytes": "^5.6.0",
    "rambda": "^6.7.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-watch-assets": "^1.0.1"
  },
  "dependencies": {
    "@liquify/cryptographer": "workspace:^0.1.0"
  }
}
