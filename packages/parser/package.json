{
  "name": "@liquify/liquid-parser",
  "version": "0.0.0",
  "description": "An incremental parser for the Liquid Templating Language",
  "scripts": {
    "dev": "rollup -c -w && pnpm test",
    "watch": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "rollup": "rollup -c",
    "pack": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --color --verbose --watch",
    "perf": "performance"
  },
  "private": false,
  "author": "ΝΙΚΟΛΑΣ ΣΑΒΒΙΔΗΣ",
  "owner": "SISSEL SAAS",
  "license": "see LICENSE.txt",
  "main": "./package/index.cjs.js",
  "module": "./package/index.es.js",
  "types": "index.d.ts",
  "ava": {
    "require": [
      "esm",
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "babelrc": true,
        "configFile": false
      }
    },
    "files": [
      "test/*.js"
    ],
    "concurrency": 5
  },
  "dependencies": {
    "@liquify/liquid-language-specs": "workspace:^0.0.0",
    "yamljs": "^0.3.0"
  },
  "repository": {
    "url": "https://github.com/panoply/liquid-parser"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@ava/babel-preset-stage-4": "^4.0.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-syntax-class-properties": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@liquify/rollup-plugin-globs": "workspace:^0.0.0",
    "@liquify/rollup-plugin-utils": "workspace:^0.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "ava": "^3.10.1",
    "babel-plugin-const-enum": "^1.0.1",
    "chalk": "^4.1.0",
    "chuhai": "^1.2.0",
    "esm": "^3.2.25",
    "performance": "^1.4.0",
    "pretty-hrtime": "^1.0.3",
    "rollup-plugin-filesize": "^9.0.2"
  }
}
