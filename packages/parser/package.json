{
  "name": "@liquify/liquid-parser",
  "version": "0.0.0",
  "description": "An incremental parser for the Liquid Templating Language",
  "scripts": {
    "dev": "rollup -c -w && pnpm test",
    "watch": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "rollup": "rollup -c",
    "pack": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --color --verbose --watch",
    "perf": "node -r esm tests/perf/parsing.js"
  },
  "private": false,
  "author": "ΝΙΚΟΛΑΣ ΣΑΒΒΙΔΗΣ",
  "owner": "SISSEL SAAS",
  "license": "see LICENSE.txt",
  "main": "./package/index.js",
  "types": "./types/export.d.ts",
  "ava": {
    "require": [
      "esm"
    ],
    "babel": {
      "testOptions": {
        "babelrc": true,
        "configFile": false
      }
    },
    "files": [
      "tests/index.js"
    ],
    "concurrency": 5
  },
  "dependencies": {
    "@liquify/liquid-language-specs": "workspace:^0.1.2",
    "line-column": "^1.0.2",
    "lodash": "^4.17.21",
    "mergerino": "^0.4.0",
    "rambda": "^6.7.0",
    "yamljs": "^0.3.0"
  },
  "repository": {
    "url": "https://github.com/panoply/liquid-parser"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@liquify/rollup-plugin-globs": "workspace:^0.0.0",
    "@liquify/rollup-plugin-utils": "workspace:^0.1.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/benchmark": "^2.1.0",
    "ava": "^3.15.0",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-preset-const-enum": "^1.0.0",
    "benchmark": "^2.1.4",
    "chalk": "^4.1.1",
    "chuhai": "^1.2.0",
    "dotenv": "^9.0.2",
    "esm": "^3.2.25",
    "performance": "^1.4.0",
    "pretty-hrtime": "^1.0.3",
    "rollup-plugin-filesize": "^9.1.1",
    "vscode-languageserver": "^7.0.0",
    "vscode-languageserver-textdocument": "^1.0.1"
  }
}
