{
  "$schema": "",
  "clients": {
    "vscode": {
      "grammar": {
        "variations": [
          {
            "variation": "standard",
            "scopeName": "source.liquid",
            "include": ["comments"],
            "exports": ["json", "cson", "yaml", "xml"]
          },
          {
            "variation": "11ty",
            "scopeName": "source.liquid.11ty",
            "include": ["comments", "frontmatter", "imports"],
            "exports": ["json", "cson", "yaml", "xml"]
          },
          {
            "variation": "jekyll",
            "scopeName": "source.liquid.jekyll",
            "include": ["comments", "frontmatter", "imports", "rouge"],
            "exports": ["json", "cson", "yaml", "xml"]
          },
          {
            "variation": "shopify",
            "scopeName": "source.liquid.shopify",
            "include": ["comments", "embedded", "imports"],
            "exports": ["json", "cson", "yaml", "xml"]
          }
        ],
        "injections": [
          {
            "scopeName": "markdown.liquid.codeblock",
            "filename": "markdown-liquid.codeblock.json",
            "include": ["comments"],
            "injectionSelector": ["L:text.html.markdown"],
            "exports": ["json", "cson"]
          },
          {
            "scopeName": "source.liquid.injection",
            "filename": "liquid.injection.json",
            "include": ["comments"],
            "exports": ["json", "cson"],
            "injectionSelector": [
              "L:source.css -comment -string -meta.embedded",
              "L:source.js -string.quoted.*.js -comment -string -meta.embedded",
              "L:source.jsx -comment -string -string.quoted.*.jsx",
              "L:source.js.jsx -comment -string",
              "L:source.ts -string -comment -string.quoted.*.ts",
              "L:source.json -string -comment -meta.embedded",
              "R:text.html.markdown -comment"
            ]
          }
        ]
      },
      "schema": [""],
      "package": {
        "includes": [""]
      }
    }
  }
}
